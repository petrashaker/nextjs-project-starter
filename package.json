{
  "name": "nextjs-project-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"*/**/*.{js,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"*/**/*.{js,ts,tsx,css,md,json}\"",
    "lint:ts": "next lint",
    "lint:css": "stylelint \"src/**/*.ts\"",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/styled-components": "^5.1.24",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "4.6.2"
  }
}
